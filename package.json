{
  "name": "electron-nowplaying",
  "version": "0.0.1",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "build": "node .electron-vue/build.ts && electron-builder",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.ts",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.ts",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": ""
  },
  "main": "./dist/electron/main.js",
  "build": {
    "productName": "Electrify",
    "appId": "build.electron.electrify",
    "files": [
      "!node_modules/babel-runtime{,/**/*}",
      "!node_modules/core-js{,/**/*}",
      "!node_modules/regenerator-runtime{,/**/*}",
      "!node_modules/deep-diff/releases{,/**/*}",
      "!node_modules/bluebird/js/browser{,/**/*}"
    ]
  },
  "build_old": {
    "productName": "electron-vue",
    "appId": "com.example.yourapp",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ]
  },
  "electronWebpack": {
    "whiteListedModules": [
      "iview"
    ]
  },
  "dependencies": {
    "@vue/cli-service": "^3.11.0",
    "axios": "^0.18.0",
    "electron-webpack": "^2.7.4",
    "regedit": "^3.0.3",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-electron": "^1.0.6",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-electron": "^1.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-typescript": "^3.11.0",
    "ajv": "^6.5.0",
    "babel-core": "^7.0.0-0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-vue": "^2.0.2",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "ejs-loader": "^0.3.3",
    "electron": "^2.0.4",
    "electron-builder": "^20.19.2",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-webpack-ts": "^3.2.0",
    "electron-webpack-vue": "^2.3.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "0.4.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.21.0",
    "typescript": "^3.5.3",
    "url-loader": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  }
}
